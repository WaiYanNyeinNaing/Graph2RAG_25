# Ollama Configuration for Bosch Graph2RAG (Local)
# Copy this to .env.ollama

HOST=0.0.0.0
PORT=9621
WEBUI_TITLE='Bosch Graph2RAG'
WEBUI_DESCRIPTION='Bosch Knowledge Graph RAG System - Local Instance'

# Directories
WORKING_DIR=./rag_storage_ollama
INPUT_DIR=./inputs

# Ollama LLM Settings
LLM_BINDING=ollama
LLM_MODEL=qwen2:latest
LLM_BINDING_HOST=http://localhost:11434
LLM_BINDING_API_KEY=

# Ollama Embedding Settings
EMBEDDING_BINDING=ollama
EMBEDDING_MODEL=nomic-embed-text
EMBEDDING_DIM=768
EMBEDDING_BINDING_HOST=http://localhost:11434
EMBEDDING_BINDING_API_KEY=

# Processing Settings
CHUNK_SIZE=1200
CHUNK_OVERLAP_SIZE=100
MAX_TOKENS=8192
TEMPERATURE=0.7
TIMEOUT=240

# Storage
LIGHTRAG_KV_STORAGE=JsonKVStorage
LIGHTRAG_DOC_STATUS_STORAGE=JsonDocStatusStorage
LIGHTRAG_GRAPH_STORAGE=NetworkXStorage
LIGHTRAG_VECTOR_STORAGE=NanoVectorDBStorage